include "Position.fbs";

namespace motis.path;

table RailEdge {
  from: ulong;
  to: ulong;

  //id: ulong; // from osm

  polyline: [Position];
  dist: ulong;
}

table RailNode {
  idx: ulong;

  id: ulong; // from osm

  pos: Position;
}

table RailGraph {
  nodes: [RailNode];
  edges: [RailEdge];
}

root_type RailGraph;
