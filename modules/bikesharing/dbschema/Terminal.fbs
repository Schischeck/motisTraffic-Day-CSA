namespace motis.bikesharing;

table Availability {
  average: double;
  median: int;
  minimum: int;
  q90: int;
  percent_reliable: double;
}

table CloseLocation {
  id: string;
  duration: int; // seconds
}

table Terminal {
  id:int;
  lat: double;
  lng: double;
  name: string;

  availability: [Availability]; // one for each hour in the week
  attached: [CloseLocation];
  reachable: [CloseLocation];
}

root_type Terminal;