include "lookup/LookupCommon.fbs";

namespace motis.lookup;

table IdEvent {
  eva_nr: string;
  type: EventType;
  train_nr: uint;
  line_id: string;
  schedule_time: ulong;  
}

table StationEvent {
  id: IdEvent;

  type: EventType;
  train_nr: uint;
  line_id: string;

  time: ulong;
  schedule_time: ulong;
}

table LookupStationEventsResponse {
  events: [StationEvent];
}