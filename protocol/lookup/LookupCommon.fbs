namespace motis.lookup;

enum EventType : byte {Arrival, Departure}

table TripId {
  // primary
  eva_nr: string;
  train_nr: uint;
  time: long;

  // secondary
  target_eva_nr: string;
  target_time: long;
  type: EventType;
  line_id: string;
}

table Station {
  eva_nr: string;
  name: string;
  lat: double;
  lng: double;
}
