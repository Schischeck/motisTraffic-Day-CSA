namespace motis.bikesharing;

// JSON example: see nextbike_itest.cc

enum AvailabilityAggregator : ubyte
  { Average, Median, Minimum, Quantile90, PercentReliable}

table BikesharingRequest {
  departure_lat: double;
  departure_lng: double;

  arrival_lat: double;
  arrival_lng: double;

  window_begin: long;
  window_end: long;

  availability_aggregator: AvailabilityAggregator = Average;
}
