max-priority-1: 0
max-priority-2: 0
max-priority-3: 0
disable-rules:
 - LongVariableName
 - UnnecessaryElseStatement
 - ShortVariableName
 - InvertedLogic
 - LongLine
 - AvoidPrivateStaticMembers
 - RedundantLocalVariable
 - UnusedLocalVariable
 - UnusedMethodParameter
 - ConstantConditionalOperator
 - ParameterReassignment
 - DoubleNegative
 - MultipleUnaryOperator
rule-configurations:
 - key: LONG_METHOD
   value: 100
 - key: CYCLOMATIC_COMPLEXITY
   value: 14
 - key: MINIMUM_CASES_IN_SWITCH
   value: 2
 - key: TOO_MANY_PARAMETERS
   value: 12
enable-clang-static-analyzer: false
